generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Book{
  id Int @id @default(autoincrement())
  title String
  // genre String
  // description String
  price Int
  author String
  publication_date DateTime @db.Date
}

